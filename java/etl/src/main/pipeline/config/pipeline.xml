<?xml version="1.0" encoding="UTF-8"?>
<pipelines
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://www.openairmarket.etl.pipeline.model"
xsi:schemaLocation="http://www.openairmarket.etl.pipeline.model file:/pipeline.xsd">

<pipeline id="productos">
  <extracts>
    <extract id="productos"/>
    <extract id="tiposProducto"/>
  </extracts>
  <transformations>
    <inputs>
      <input id="productos"/>
    </inputs>
    <conversions>
      <conversion id="productos"/>
    </conversions>
  </transformations>
</pipeline>

<extracts path="script/extraction/">
  <extract id="productos">
    <scripts>
      <script outputName="productos.csv">extract_productos.sql</script>
    </scripts>
  </extract>
  <extract id="tiposProducto">
    <scripts>
      <script outputName="tipos_producto.csv">extract_tipos_producto.sql</script>
    </scripts>
  </extract>
</extracts>

<inputs path="script/input/">
  <input id="productos" table="producto" description="Load productos csv">
    <scripts>
      <script>input_productos.sql</script>
    </scripts>
  </input>
</inputs>

<conversions path="script/conversion/">
  <conversion id="productos">
    <scripts>
      <script outputName="productos.csv">convert_productos.sql</script>
    </scripts>
  </conversion>
</conversions>
</pipelines>